<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (loggedin) { %>
    <a href="/account">
      <% if (accountData.account_type === 'Client') { %> Welcome Basic <% } else {
      %> Welcome <%= accountData.account_type %> <% } %>
    </a> | 

    <a href="/account/logout">Logout</a>

    <% } else { %>
    <a href="/account/login">My Account</a>
    <% } %>
  </div>
</header>
